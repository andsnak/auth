{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "Node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist" ,
    "removeComments": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,

    "types": [
      "node"
    ],

    "baseUrl": "./",
  },
}
